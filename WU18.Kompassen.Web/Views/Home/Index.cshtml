<script>
    $("#panel2").hide();
    $("#panel3").hide();
    $("#navKurser").on("click", function () {



        $("#panel1").show();
        $("#panel2").hide();
        $("#panel3").hide();

      
     
            

    });

    $("#navStudenter").on("click", function () {



