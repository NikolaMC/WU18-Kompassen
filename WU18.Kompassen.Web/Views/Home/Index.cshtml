                                <th scope="col">År</th>
                                <th scope="col">Aktiv/Inaktiv</th>
                            </tr>

                        </thead>

                        <tbody id="coursesTable1"></tbody>

                    </table>

                </div><!--col-lg-10 end-->
            </div><!--row end-->
